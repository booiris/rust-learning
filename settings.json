        "rust": "rustc $fileName -o $workspaceRoot/build/$fileNameWithoutExt && $workspaceRoot/build/$fileNameWithoutExt",
    },
    "code-runner.saveFileBeforeRun": true,
    "code-runner.preserveFocus": false,
    "github.copilot.enable": {
